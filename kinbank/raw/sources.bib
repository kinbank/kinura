@article{bostoen_subject_2012,
    Abstract = {In this paper it is demonstrated that the marking of argument focus in Mbuun (B87), a western Bantu language from the Democratic Republic of the Congo (DRC), relies on syntactic and morphological devices that deviate from common tendencies reported in eastern and southern Bantu languages. Focalising a non-verbal constituent in Mbuun obligatorily involves deviations from the canonical SVO order. A focused object is fronted immediately before the verb in Mbuun, resulting in a SOV word order, which runs counter to the narrow focus site immediately after the verb in many other Bantu languages. The object also moves in Mbuun when other non-verbal clause constituents are focused. Both subjects and oblique arguments are focused in situ but their focalisation triggers a movement of the object to clause-initial position resulting in OSV. Morphologically speaking, Mbuun argument focus is peculiar, because it involves a Class 1 á- versus ká- allomorphy in the verbal subject-concord slot, which co-varies not only with focus, but also with tense/aspect. This morphological focus device has not been reported in eastern and southern Bantu, where the so-called ‘conjoint/disjoint’ distinction in the Tense-Aspect-Mood (TAM) slot is more common. It is, however, a wider western Bantu feature, which may have its origin in an identification copula.},
    Author = {Bostoen, Koen and Mundeke, Leon},
    Doi = {10.2989/16073614.2012.737588},
    Issn = {1607-3614},
    Journal = {Southern African Linguistics and Applied Language Studies},
    Keywords = {AC33, LH, kinbank, mpuo1241},
    Month = {June},
    Number = {2},
    Pages = {139--154},
    Title = {Subject marking, object-verb order and focus in {Mbuun} ({Bantu}, {B87})},
    Url = {https://doi.org/10.2989/16073614.2012.737588},
    Urldate = {2019-01-24},
    Volume = {30},
    Year = {2012},
    sourceid = {AC33}
}

@article{de_sousberghe_cousins_1965,
    Abstract = {Cross-Cousins and Their Descendants. In a large region of Central Africa the marriage of cross-cousins is a preferred union. This paper distinguishes two patterns of kinship rights and obligations attributed to the children of cross-cousins. According to one pattern, the preferred union is presumed to have taken place between all cross-cousins of opposite sex whether this has been the case or not. The preferred marriage is in practice rare. Even in the cases where cross-cousins have not married one of their preferred partners, their offspring treat one another as if their respective parents had been married and as if they themselves were siblings. Marriage is prohibited between them, kinship terminology is correspondingly adjusted to their sibling status, and they transmit the cross-cousin relationship to their own offspring exactly as if they were true siblings. It follows that each generation of descendants of a sibling pair consists alternatively of siblings or of cross-cousins; prohibition on marriage alternates with preference for marriage. By contrast, the other pattern is based on the idea that the preferred union between cross-cousins is merely possible. In these cases, the offspring of cross-cousins who have not intermarried are distinguished from the offspring of those who have, since the latter are true siblings and the former count as cross-cousins between whom marriage is permitted. Thus the relationship of cross-cousins is transmitted down the generations as a permission to intermarry. The author assembles the information for establishing the existence of these two patterns of kinship and analyses the variations from one tribe to another. It is noteworthy that the two patterns do not coincide with the distribution of unilineal descent in the region. The patrilineal Burundi must be classed with the matrilineal Pende, Holo, and Songo, while the patrilineal Rwanda have a pattern similar to that of the matrilineal Southern Yaka. The pattern of presumed cross-cousin marriage is often associated with a preferred marriage across the generations between classificatory grandparent (male) and grandchild (female) which is widely recorded in Central Africa. Presumed cross-cousin marriage also creates a bar against marriage within every alternate generation of cousins--those who find themselves classed as siblings. The author extends this framework to interpret some hitherto intractable material about preferred cousins in the Congo. Between cross-cousins and their descendants marriage is only permitted between equal generations, that is, within ego's own generation or between alternate generations. But in a number of tribes, Bakongo, Basongo, Yansi, and Mbuun, a kind of converse is applied: between parallel cousins and their descendants marriage is permitted between unequal generations. Inequality of generation cancels the prohibitions which are transmitted between generations.},
    Author = {De Sousberghe, L.},
    Doi = {10.2307/1157662},
    Issn = {0001-9720},
    Journal = {Africa: Journal of the International African Institute},
    Keywords = {AC35, LH, kinbank, mpuo1241, phen1239, yaka1269, yans1239},
    Number = {4},
    Pages = {396--421},
    Shorttitle = {Cousins {Croisés} et {Descendants}},
    Title = {Cousins {Croisés} et {Descendants}: {Les} {Systèmes} du {Rwanda} et du {Burundi} {Comparés} à {Ceux} du {Bas}-{Congo}},
    Url = {http://www.jstor.org/stable/1157662},
    Urldate = {2017-09-15},
    Volume = {35},
    Year = {1965},
    sourceid = {AC35}
}

